<div class="users-text-container">
<div class="container">
  <div class="text-center">
    <h1>User Information</h1>
  </div>
</div>
<div class="container">
<table class="table table-bordered table-striped table-container">
  <thead>
    <tr>
    <th>Email</th>
    <th>Admin?</th>
    <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email%></td>
        <td><%= user.admin?%></td>
       <td>
       <% if current_user.admin? && !current_user.nil? %>
       <%= link_to "delete", user, method: :delete,
                              data: { confirm: "You sure?" } %>
  <% end %>
  </td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
</div>
</div>